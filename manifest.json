{
    "manifest_version": 3, 
    "name": "Youtube Notion", 
    "description": "find global entry interviews",
    "version" : "1.0",
    "action" : { 
        "default_icon" : "assets/delete.png"
        
    },
     "icons": {
        "16": "assets/yt-16.png",
        "32": "assets/yt-32.png",
        "48": "assets/yt-48.png",
        "128": "assets/yt-128.png"
        },
        "background" : {
            "service_worker" : "background.js",
            "type": "module"
        }, 

        "permissions": [
            "storage"
        ]
}
